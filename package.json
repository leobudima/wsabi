  "bugs": {
    "url": "https://github.com/MCProHosting/wsabi/issues"
  },
  "homepage": "https://github.com/MCProHosting/wsabi",
  "dependencies": {
    "boom": "^4.1.0",
    "cookiejar": "^2.0.6",
    "hoek": "^4.1.1",
    "socket.io": "^2.0.3"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^5.0.0",
    "hapi": "^13.0.0",
    "istanbul": "^0.4.2",
    "mocha": "^2.2.4",
    "sinon": "^1.14.1"
